/* 新增 */
.clear {clear: both;height: 0px;line-height: 0px;font-size: 0px;overflow: hidden;visibility: hidden; }
.clearfix:after {content: "";display: block;height: 0;clear: both;visibility: hidden; }
.fl{float: left;}
.fr{float: right;}
.lr1{overflow: hidden;text-overflow: ellipsis;-ms-text-overflow:ellipsis;white-space:nowrap;}
.lr2{overflow: hidden;text-overflow: ellipsis;display: -webkit-box;-webkit-box-orient: vertical;-webkit-line-clamp: 2;}
.lr3{overflow: hidden;text-overflow: ellipsis;display: -webkit-box;-webkit-box-orient: vertical;-webkit-line-clamp: 3;}

/* loadding */
.loadding{
    width:36rpx;
    height: 36rpx;
    margin: 0 auto;
    margin-top: 180rpx;
}
.loadding image{
    display: block;
    width: 100%;
    height: 100%;
}

/* empty */
.empty{
    padding-top: 100rpx;
    background: #fff;
  }
  .empty text{
    display: block;
    width: 100%;
    text-align: center;
  }
  .empty .empty-icon{
    width: 260rpx;
    height: 260rpx;
    background: #DFE0E8;
    border-radius: 100%;
    position: relative;
    margin: 0 auto;
    margin-bottom: 50rpx;
  }
  .empty .empty-icon image{
    width: 150rpx;
    height: 150rpx;
    left: 50%;
    top: 50%;
    position: relative;
    transform: translate(-50%,-50%);
  }
  /* 分割块 */
  .delblock{
    width:100%;
    height:20rpx;
    background:#F8F8F8;    
  }
  .deltriangle{
    width: 18rpx;
    height: 18rpx;
    border-top:1px solid #888; 
    border-left:1px solid #888; 
    transform: rotate(135deg);
  }

  /* 版权 */
  .weui-footer {
    color:#999;
    font-size:14px;
    text-align:center;
    padding:40px 0px 20px 0px;
    display:flex;
    justify-content:center;
    align-items:center;
}
.weui-footer__link {
    display:inline-block;
    vertical-align:top;
    margin:0 .62em;
    position:relative;
    font-size:13px;
    color:#7f7f7f;
}
.weui-footer__text{
    padding:0 .34em;
    font-size:13px;
    color:#7f7f7f;
}

/* 客服 */
.com-widget-goTop { 
    position: fixed; 
    bottom: 125px; 
    right: 5px; 
    /* background: rgba(0,0,0,0.48);  */
    background: #F3514F;
    border-radius: 50%; 
    overflow: hidden; 
    z-index: 999; 
    } 
    .com-widget-goTop .icon-gotop{ 
    background-color: rgba(0,0,0,0.8); 
    display: inline-block; 
    width: 50px; 
    height: 50px; 
    line-height: 68px; 
    font-size: 11px; 
    color: #ffffff; 
    text-align: center; 
    border-radius: 50%; 
    background: url(data:image/png;base64,) no-repeat center 3px; 
    -webkit-background-size: 30px auto; 
    }
    .guest-button{
      position: absolute;
      top:0;
      left:0;
      opacity: 0;
    }