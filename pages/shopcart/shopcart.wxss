.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  box-sizing: border-box;
  padding: 0;
}
page{
  background: #F6F6F9;
}
text,view{
   font-family: Microsoft yahei;
  font-size: 14px;
}
.none-goods{
  padding-top: 100rpx;
}
.none-goods text{
  display: block;
  width: 100%;
  text-align: center;
}
.none-goods .cart-icon{
  width: 260rpx;
  height: 260rpx;
  background: #DFE0E8;
  border-radius: 100%;
  position: relative;
  margin: 0 auto;
  margin-bottom: 50rpx;
}
.none-goods .cart-icon image{
  width: 150rpx;
  height: 150rpx;
  left: 50%;
  top: 50%;
  position: relative;
  transform: translate(-50%,-50%)
}
.cart-goods-block{
  width: 100%;
  background:#fff;
  margin: 20rpx 0rpx 100rpx 0rpx;
}
.cart-goods-block .cart-goods-item{
   display: flex;
  border-bottom: 1px solid #f6f6f9;
  padding: 30rpx 20rpx 30rpx 0rpx;
  align-items:flex-start;
}
.cart-goods-block .cart-goods-item i{
  display: flex;
  width: 40rpx;
  height: 40rpx;
  background:url(../images/iconfont-gou.png) center center no-repeat #ec5151;
  background-size: 70%;
  border-radius:50%; 
  margin-right: 10rpx;
}
.cart-goods-block .cart-goods-item image{
  width: 200rpx;
  height: 200rpx;
}
.cart-goods-block .cart-goods-item .pro text{
  display: block;
  height: 45rpx;
  margin-bottom: 5rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.cart-goods-block .cart-goods-item .pro .goods_name{
  width: 100%;
  display: block;
  color: #333;
}
.cart-goods-block .cart-goods-item .pro .goods_price{
  width: 100%;
  display: block;
  color: #EC5151;
}
.cart-goods-block .cart-goods-item .pro .goods_detail{
  width: 100%;
  display: block;
  color: #999;
  font-size: 22rpx;
}
.cart-goods-block .cart-goods-item .pro .num_option{
    overflow: hidden;
   border: 1px solid #ededed;
    border-radius: 4px;
    width: 220rpx;
    height: 60rpx;
    display: flex;
}
.cart-goods-block .cart-goods-item .pro .num_option .num_minus{
 float: left;
 width: 70rpx;
 height: 60rpx;
 text-align: center;
 line-height: 60rpx;
}
.cart-goods-block .cart-goods-item .pro .num_option .num_display{
 float: left;
  border-left: 1px solid #ededed;
  border-right: 1px solid #ededed;
  text-align: center;
 width: 70rpx;
 height: 60rpx;
 line-height: 60rpx;
}
.cart-goods-block .cart-goods-item .pro .num_option .num_plus{
 float: left;
  text-align: center;
  width: 70rpx;
  height: 60rpx;
  line-height: 60rpx;
}
.cart-goods-block .cart-goods-item .pro .delete-btn{
  position: absolute;
  right: 20rpx;
  bottom: 0;
  border: 1px solid #EC5151;
  padding: 10rpx 15rpx;
  border-radius: 5px;
  color: #EC5151;
  font-size: 24rpx;
}
.cart-goods-block .cart-goods-item .pro{
  height: 200rpx;
  width: 430rpx;
  margin-left: 20rpx;
  position: relative
}
.cart-goods-block .shop-name{
  display: flex;
  border-bottom: 1px solid #f6f6f9;
  padding: 30rpx 20rpx;
  align-items:center;
}
.cart-goods-block .shop-name i{
  display: flex;
  width: 40rpx;
  height: 40rpx;
   background:url(../images/iconfont-gou.png) center center no-repeat #EC5151;
  background-size: 70%;
  border-radius:50%; 
  margin-right: 10rpx;
}
.cart-goods-block .shop-name text{
  display: flex;
  width: 90%;
  font-size: 16px;
}
.cart-bottom-bar{
  position: fixed;
  bottom:0;
  left: 0;
  width: 100%;
  align-items:center;
  background: #fff;
  border-top: 1px solid #e7e7e7;
  height: 100rpx;
  line-height: 100rpx;
}
.cart-bottom-bar .sel-text{
  margin:0rpx 20rpx 0rpx 10rpx;
}
.cart-bottom-bar .all-sel{
  width: 40rpx;
  height: 40rpx;
   background:url(../images/iconfont-gou.png) center center no-repeat #EC5151;
  background-size: 70%;
  border-radius:50%; 
  margin-right: 10rpx;
}
.cart-bottom-bar .total-price{
  margin-right: 20rpx;
}
.cart-bottom-bar .title{
  font-size: 16px;
}
.cart-bottom-bar .total-price .num{
  color: #EC5151;
  font-size: 16px;
}
.myicon{
  /* padding-top: 60rpx; */
  color:#EC5151;
  font-size: 16px;
  /* margin-right: 15rpx;
  padding-left: 20rpx; */
  padding:60rpx 15rpx 0rpx 20rpx;
}
.gopay{
  width: 180rpx;
  line-height: 100rpx;
  height: 100rpx;
  text-align: center;
  color:#fff;
  font-size: 16px;
  border-radius: 0px;
}
.l-mycheck{
  margin-top: 20rpx;
}
.cart-bottom-lf{
  margin-left: 10rpx;
}
