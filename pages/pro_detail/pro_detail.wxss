.delblock1{
    margin-bottom:70rpx;
}
text{
  font-family: Microsoft yahei;
  font-size: 14px
}
view{
    font-family: Microsoft yahei;
  font-size: 14px
}
.page{
    background: #fff;
    padding-bottom: 40rpx;
}
.swiper-block {
  height:750rpx;
    width:750rpx;
}

.swiper-block swiper{
    height: 750rpx;
}
.swiper-block image{
    width: 100%;
    height:100%
}
.goods-detail-block{
    width: 100%;
    background: #fff;
    /* margin-top:15rpx; */
    padding:20rpx 0;
}
.proInfo-block{
    width: 92%;
    margin: 20rpx auto;
    background: #fff
}
.proInfo-block .goodsTitle{
    font-size: 35rpx;
    margin-bottom: 20rpx
}
.proInfo-block .shopPrice{
    margin-bottom: 20rpx;
}
.proInfo-block .shopPrice text{
    color: #EC5151;
    font-size: 34rpx;
}
.proInfo-block .shopPrice .discount{
    font-size: 22rpx;
    background: #EC5151;
    color: #fff;
    padding: 0 4px;
    border-radius: 8px;
    margin-left: 10rpx;
}
.proInfo-block .marketPrice{
    color: #888;
    font-size: 34rpx;
    margin-bottom: 20rpx;
}
.proInfo-block .marketPrice .price_num{
    color: #aaa;
    text-decoration: line-through
}
.proInfo-block .dis-box{
    overflow: hidden;
    /* padding-bottom: 30rpx; */
}
.proInfo-block .dis-box text{
    width: 50%;
    float: left;
    color: #999;
    font-size: 28rpx
}


.bottom-block{
    position: fixed;
    left: 0;
    bottom: 0;
    z-index: 6;
    background: #fafafc;
    border-top: 1px solid #efefef;
    width: 100%;
    height:50px;
    vertical-align: middle;
    display: flex;
}
.bottom-block .icon-fa{
    width:36%;
}
.bottom-block .icon-b{
    display:inline-block;
    width:50%;
    text-align: center;
    position: relative;
    top:0;
    height:43px;
    background:#fafafc;
    padding-top:14rpx;
}
.bottom-block .icon-b .cart-num{
    position: absolute;
    right: 25rpx;
    top: 6rpx;
    width: 35rpx;
    height: 35rpx;
    font-size: 22rpx;
    line-height: 35rpx;
    background: #EC5151;
    color: #fff;
    border-radius:50% ;
}
.bottom-block .icon-b .iconfont{
    font-size:40rpx;
}
.bottom-block .icon-b .red{
    color:red;
}
.bottom-block .shopping-cart text{
    display: block;
    text-align: center;
    width: 100%;
    font-size: 12px;
}
.bottom-block .btn-cart{
    width:32%;
    height:50px;
    line-height:50px;
    display: inline-block;
    font-size: 30rpx;
    border-radius: 0;
}
.bottom-block .buymust{
    background: #d81e06;
}
.goods-buy-block{
    background: #fff;
     /* margin-top: 15rpx; */
    width: 100%;
     padding: 10rpx 0;
}
.goods-buy-block .item-block{
    overflow: hidden;
    padding: 15rpx 4%;
    background: #fff;
    width: 92%; 
    margin-bottom: 2rpx;
    display: flex
}
.goods-buy-block .item-block text{
    display: block;
    font-size: 15px;
}
.goods-buy-block .item-block text.left{
   margin-right: 10rpx;
    color: #777;
}
/*产品参数*/
.product-parameter{
    background: #fff;
    padding:0 4% 10rpx 4%;
    /* margin-top:15rpx; */
    border-top:1px solid #eee;
    margin-bottom:50px;
}
.product-parameter .parameter-title{
    padding:30rpx 0;
    padding-bottom:10rpx;
    /* border-bottom:1px solid #eee; */
}
.product-parameter .parameter-title text{
    font-size:30rpx;
    color:#777;
}
.parameter-content{
    padding:25rpx 0;
    border-bottom:1px solid #eee;
    font-size:28rpx;
    display: flex;
}
.product-parameter .last{
    border-bottom:none;
}
.parameter-content .parameter-key{
    width:35%;
}
.parameter-content .parameter-value{
    width:65%;
    padding-right:20rpx;
}
.parameter-content .parameter-value {
    color:#777;
}
/*商品评价  */
.comment{
  background: #fff;
  margin-top:20rpx;
  margin-bottom:60rpx;
}
.comment-head{
  padding:20rpx 4%;
  font-size:16px;
  border-bottom:1px solid #ddd;
}
.comment-content{
  margin:15rpx 4%;
  border-bottom:1px solid #ddd;
  color:#666;
}
.content-head{
  margin-bottom:10rpx;
}
.content-head .right{
  float:right;
}
.top-nav-bar{
    width: 80%;
    padding: 0 10%;
    background: #EC5151;
    color: #fff;
    display: flex;
}
.top-nav-bar .item{
    display: flex;
    flex-direction: column-reverse;
    flex: 1;
    text-align: center;
    padding: 30rpx 0;
    font-size: 16px;
    position: relative
}
.top-nav-bar .item.selected:after{
        content: " ";
    position: absolute;
    left: 2rem;
    right: 2rem;
    bottom: 0;
    height: 1.5px;
    background: #fff;
    z-index: 2;
}
.mask-filter-div{
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background:rgba(0,0,0,.4);
}
.standard-block{
    position: fixed;
    bottom: 100rpx;
    left: 0;
    width: 100%;
    padding: 20rpx 0;
    background: #fff;
    z-index: 111;
}
.standard-block .pro-img{
    width: 300rpx;
    height: 200rpx;
}
.standard-block .pro-info{
    margin-left: 20rpx;
}
.standard-block .pro-info text{
    display: block;
}
.standard-block .pro-info .pro-title{
    height: 80rpx;
    overflow: hidden;
    font-size: 30rpx;
    margin-bottom: 20rpx;
}
.standard-block .pro-info .pro-price{
    color: #EC5151;
    font-size: 35rpx;
     margin-bottom: 20rpx;
}
.standard-block .pro-info .pro-total{
    color: #999;
}
