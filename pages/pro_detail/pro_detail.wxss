.standard-block  .close-btn{
    position: absolute;
    width: 24px;
    height: 24px;
    right: 20rpx;
    top: -12px;
    border-radius: 50%;
    background: #fff;
    padding:3px;
}
.standard-block .close-btnson{
    color: #666;
    width: 21px;
    height: 21px;
    font-size: 18px;
    border-radius: 50%;
    border: 1px solid #666;
    line-height: 21px;
    text-align: center;
}
.standard-block .pro-img image{
    width: 100%;
    height: 100%;
}
.goods-info{
    display: flex;
    padding: 20rpx 40rpx;
    border-bottom:1px solid #888; 
}
.change-standard{
    height: 500rpx;
    padding: 0 4%;
    width: 92%;
}
.change-standard .info-block{
    width: 100%;
}
.change-standard .num-option-box{
    width: 300rpx;
    display: flex;
    border: 1px solid #ededed;
    border-radius: 4px;
}
/* .change-standard .num-option-box .title{} */
.change-standard .num-option-box view{
    width: 36px;
    height: 36px;
    text-align: center;
    border: none;
    background: none;
    line-height: 30px;
    font-size: 30px;
    font-weight: lighter;
    color: #888;
}
.change-standard .num-option-box input{
    border: 0;
    border-left: 1px solid #ededed;
    border-right: 1px solid #ededed;
    border-radius: 0;
    padding: 2px 4px;
    height: 32px;
    text-align: center;
    flex: 1
}
.change-standard .info-block .title{
    color: #777;
    display: block;
    font-size: 30rpx;
    margin: 20rpx 0;
}
.change-standard .info-block .detail{
    display: flex;
    flex-wrap:wrap;
    align-content:space-between;
}
.change-standard .info-block .detail .detail-block.active{
    border: 1px solid #EC5151;
    color: #EC5151;
}
.change-standard .info-block .detail .detail-block{
    padding: 15rpx 30rpx;
    display: block;
    text-align: center;
    border: 1px solid #efefef;
    border-radius: 4px;
    color: #666;
    margin-right: 20rpx;
    margin-bottom:20rpx;
}
