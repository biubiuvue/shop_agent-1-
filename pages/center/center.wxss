.header-box{
    /* height:410rpx; */
    display: flex;
    justify-content: center;
    align-items: center;
    background: #fff;
}
.section{
    flex:1;
}
.user-header-pic image {
    width:90px;
    height:90px;
    border-radius:50%;

}
.user {
   color:#fff;
}
.user .username {
    font-size:14px;
    color:#333;
    text-align: center;
}
.user .userlevel {
    font-size:12px;
    color:#333;
    text-align: center;
}
.user .useredit {
   position:absolute;
   right:30rpx;
   top:190rpx;
}
.user-nav-box  {
    width: 100%;
    padding:60rpx 0rpx 40rpx 0rpx;
    font-size:16px;  
    color: #333;
    display: flex;
}
.user-nav-box .nav-left {
    text-align:center;
    flex:1;
}
.user-nav-box .nav-right {
    text-align:center;
    flex:1; 
}
.tiancong {
    height:10px;
    width:100%;
    background:#F6F6F9;
}
.order {
    height:40px;
    line-height:40px;
    border-bottom:1px solid #F6F6F9;
    padding: 10rpx 30rpx;
    display: flex;
    justify-content: center;
    background: #fff;
}
.order .left-item{
    display: flex;
    flex: 1;
    align-items: center;
}
.order .right-item{
    display: flex;
    flex: 1;
    align-items: center;
    justify-content: flex-end;
}
.order-icon {
    display: flex;
   width:40rpx;
   height:40rpx;
   
}
.order .myorder {
    display: flex;
}
.order .arrow {
    float:right;
    width:10px;
    height:10px;
    border-top:1px solid #939393;
    border-right:1px solid #939393;
    transform:rotate(45deg);
    -ms-transform:rotate(45deg); 	/* IE 9 */
    -moz-transform:rotate(45deg); 	/* Firefox */
    -webkit-transform:rotate(45deg); /* Safari å’Œ Chrome */
    -o-transform:rotate(45deg); 
    display: flex;
}
.order .myorder-all {
   display: flex;   
   color:#888888;
}
.mydingdan {
   display:flex;
    justify-content:center;
    width:100%;
    padding: 50rpx 0;
    background: #fff;
}
.mydingdan .mydingdan-list {
    padding: 0 10rpx;
    border-right: 1px solid #F6F6F9;
    flex: 1;
    position:relative;
}
.mydingdan-list .dingdan-icon image{
    width: 100%;
    height: 100%
}
.mydingdan-list .dingdan-icon {
    width:30px;
    height:30px;
    margin:0 auto;   
}

.mydingdan-list .num{
    text-align:center;
    background:#EC5151;
    color:#fff;
    width:15px;
    height:15px;
    line-height:15px;
    border-radius:11px;
    position:absolute;
    right:32px;
    top:-5px;
    font-size:12px;    
}
.mywallet {
     padding: 0 10rpx;
    border-right: 1px solid #F6F6F9;
    flex: 1;
    text-align:center;
}
.mywallet .wallet-top {
    color:#EC5171;
    font-size:20px;
}
.mydingdan text {
    font-size:14px;
    color:#333;
    display: block;
    text-align: center;
}
.user-header-pic {
    text-align:center;
    width:180rpx;
    margin: 0 auto;
}
.weui-footer_fixed-bottom{
    background: #f8f8f8;
}
.header-content{
    width: 100%;
}