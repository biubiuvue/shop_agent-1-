.page{
  background: #F6F6F9;
}
text,view{
   font-family: Microsoft yahei;
  font-size: 14px;
}
.top-nav-bar{
    width: 80%;
    padding: 0 10%;
    background: #EC5151;
    color: #fff;
    display: flex;
    position: relative;
}
.top-nav-bar .back-btn{
  display: block;
  position: absolute;
  width: 25rpx;
  height: 25rpx;
  border-top:2px solid #fff; 
  border-left:2px solid #fff; 
  left: 8%;
  top: 50%;
  z-index: 222;
  transform: translate(-50%,-50%) rotate(-45deg);
}
.top-nav-bar .item{
    display: flex;
    flex-direction: column-reverse;
    flex: 1;
    text-align: center;
    padding: 30rpx 0;
    font-size: 16px;
    position: relative
}
.address-manager{
  width: 90%;
  padding: 30rpx 5%;
   background:#fff; 
  background-size: 150rpx auto;
  position: relative;
}
.address-manager .arrow-right{
  display: block;
  position: absolute;
  width: 20rpx;
  height: 20rpx;
  border-top:1px solid #888; 
  border-left:1px solid #888; 
  right: 5%;
  top: 50%;
  z-index: 111;
  transform: translate(-50%,-50%) rotate(135deg);
}
.address-manager .addressee{
  margin-bottom: 20rpx;
}
.address-manager .address-info{
  display: flex;
  align-items: center;
}
.address-manager .address-info text{
  display: flex;
  color: #777;
  margin-right: 20rpx;
 
}
.address-manager .addressee text{
  margin-right: 10rpx;
  font-size: 16px;
}
.goods-item{
  width: 100%;
  padding: 10rpx 0;
  background: #fff;
  margin: 20rpx 0;
}
.goods-item .shop-name{
  width: 90%;
  padding: 30rpx 5%;
  font-size: 35rpx;
      border-bottom: 1px solid #f6f6f9;
}
.goods-item .goods-info image{
  width: 240rpx;
  height: 240rpx;
  display: flex;
  margin-right: 20rpx;
  border: 1px solid #F6F6F9;
}
.goods-item .goods-info{
  width: 92%;
  margin: 0 4%;
  display: flex;
  align-items: flex-start;
   border-bottom: 1px solid #f6f6f9;
   padding: 30rpx 0;
}
.goods-item .goods-info .goods-info-de{
  flex: 1;
}
.goods-item .goods-info .goods-info-de .goods-name{
  display: block;
  width: 100%;
  height: 80rpx;
  overflow: hidden;
  font-size: 29rpx;
  margin-bottom:20rpx;
}
.goods-item .goods-info .goods-info-de .goods-price-num{
  width: 100%;
  display: flex;
  margin-top:20rpx;
}
.goods-item .goods-info .goods-info-de .goods-price-num text{
  flex: 1;
}
.goods-item .goods-info .goods-info-de .goods-price-num .goods-price{
  color: #EC5151;
}
.goods-item .goods-info .goods-info-de .goods-price-num .goods-num{
  text-align: right;
}
.express-style{
  width: 90%;
  padding: 30rpx 5%;
  margin-top: 20rpx;
  margin-bottom:50px;
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  border-bottom: 1px solid #f6f6f9;
  position: relative;
  background: #fff;
}
.express-style .express-content{
  width:100%;
  display:flex;
  padding:20rpx 0;
  border-bottom:1px solid #eee;
}
.express-style .last-content{
  border-bottom:none;
}
.express-style .title{
  display: flex;
  flex: 1;
  color: #777;
}
.express-style .express-detail{
  display: flex;
  flex: 1;
  justify-content:flex-end;
}
/* .express-style .express-detail .express-name{} */
.express-style .express-detail .express-price{
  color: #EC5151;
}
.express-style text{
  margin-right: 30rpx;
}
.express-style .arrow-right{
  display: block;
  position: absolute;
  width: 20rpx;
  height: 20rpx;
  border-top:1px solid #888; 
  border-left:1px solid #888; 
  right: 0;
  top: 50%;
  z-index: 111;
  transform: translate(-50%,-50%) rotate(135deg);
}
.total-num-price{
  width: 92%;
  margin: 0 4%;
  display: flex;
  align-items: center;
   border-bottom: 1px solid #f6f6f9;
   padding: 30rpx 0;
   position: relative;
   justify-content: flex-end;
}
.total-num-price text{
  color: #777;
  font-size: 15px;
}
.total-num-price .num{
  display: flex;
}
.total-num-price .price{
  display: flex;
}
.total-num-price .price text{
  display: flex;
}
.total-num-price .price .price-num{
  display: flex;
  flex: 1;
  color: #EC5151;
}
.pay-style-box{
  width: 90%;
  padding: 30rpx 5%;
  background: #fff;
  margin-top: 20rpx;
  display: flex;
  position: relative;
}
.pay-style-box .title{
  flex: 1;
}
.pay-style-box .pay-style-detail{
  color: #EC5151
}
.pay-style-box .arrow-right{
  display: block;
  position: absolute;
  width: 20rpx;
  height: 20rpx;
  border-top:1px solid #888; 
  border-left:1px solid #888; 
  right: 20rpx;
  top: 50%;
  z-index: 111;
  transform: translate(-50%,-50%) rotate(135deg);
}

.cart-bottom-bar button{
  font-size: 15px;
  margin-right: 10rpx;
}
.cart-bottom-bar{
  position: fixed;
  bottom:0;
  left: 0;
  width: 95%;
  z-index: 1111;
  background: #fafafc;
  border-top: 1px solid #efefef;
  padding: 20rpx 2.5%;
}
.cart-bottom-bar .cancel-btn{
  font-size: 30rpx;
  background: #EC5151;
  color: #fff;
  width:210rpx;
  float:right;
  margin-left:25rpx;
}
.cart-bottom-bar .pay-btn{
  font-size: 30rpx;
  background: #F5751D;
  color: #fff;
  width:210rpx;
  float:right;
}
.cart-bottom-bar .back-btn{
  font-size: 30rpx;
  background: #EC5151;
  color: #fff;
  width:210rpx;
  float:right;
  margin-left:25rpx;
}
.optionList{
  margin-top: 20rpx;
}
/* .optionList text{} */
.dispatchPicker{
  width: 30%;
  margin-right: 30rpx;
  text-align: right;
}
/*蒙板支付*/
.mask-filter-div{
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background:rgba(0,0,0,.4);
}
.pay-block{
  position: fixed;
  bottom: 0;
  left:0;
  width: 100%;
  /*left:49rpx;*/
  /*padding: 20rpx;*/
  background: #fff;
  z-index: 111;
}
.pay-block .pricePay{
  text-align: center;
  font-size: 46rpx;
  padding:50rpx 0;
}
.pay-block .pricePay text{
  font-size: 46rpx;
  color:#000;
}
.pay-block .pricePay text span{
  font-weight: 600;
}
.pay-block .orderInfoPay{
  margin-left:26rpx;
  padding:30rpx 20rpx 24rpx 0;
  border-bottom:1px solid #eee;
}
.pay-block .orderInfoPay .left{
  color:#666;
}
.pay-block .orderInfoPay .right{
  float:right;
  color: #333;
}
.pay-block .pro-title{
  text-align: center;
  font-size:30rpx;
  padding:24rpx 0;
  border-bottom:1px solid #eee;
}
.pay-block .wx-pay{
  height:85rpx;
  width:94%;
  line-height:85rpx;
  color:#fff;
  border-radius: 3px;
  margin-top:340rpx;
  margin-bottom:30rpx;
  font-size: 30rpx;
}
.pay-block  .close-btn{
  position: absolute;
  width: 24px;
  height: 24px;
  right: 20rpx;
  top: -12px;
  border-radius: 50%;
  background: #fff;
  padding:3px;
}
.pay-block  .close-btnson{
  color: #666;
  width: 21px;
  height: 21px;
  font-size: 18px;
  border-radius: 50%;
  border: 1px solid #666;
  line-height: 21px;
  text-align: center;
}