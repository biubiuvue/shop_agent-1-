/* partner.wxss */
.page{
    font-family: "Microsoft yahei";
}
.search{
    height:110rpx;
}
.search .position{
    height:110rpx;
    padding:20rpx;
    display: flex;
    position: absolute;
}
.search .icon-search{
    position: absolute;
    width:40rpx;
    height:70rpx;
    left:24rpx;
    top:19rpx;
    background:#fff url(../../public/images/search.png) no-repeat left center ;
}
.search input{
    /*background: #fff;*/
    box-sizing: border-box;
    padding-left: 50rpx;
    height:70rpx;
    font-size:24rpx;
    border-radius: 3px;
    background:#fff ;
    width:610rpx;
}
.search .search-btn{
    display: inline-block;
    width:100rpx;
    height:70rpx;
    background: #ec5151;
    line-height: 70rpx;
    color:#fff;
    text-align: center;
    font-size:28rpx;
    border-radius:3px;
}
.header-tap{
    box-sizing: border-box;
    background: #fff;
    padding:20rpx 0;
    width:100%;
    display:flex;
    border-top:1px solid #eee;
    border-bottom: 1px solid #eee;
}
.header-tap view{
    width:33%;
    text-align: center;
    border-right:1px solid #eee;
}
.header-tap view.last{
    border-right:none;
}
.header-tap view image{
    width:42rpx;
    height:36rpx;
    line-height:36rpx;
    margin-right:10rpx;
    background-image: url("data:image/png;base64,");
}
.header-tap view.active image{
    background-image: url("data:image/png;base64,");
}
.header-tap view text{
    font-size:28rpx;
    color:#666;
}
.header-tap view.active text{
    color:#ec5151;
}
.p-list {
    position: absolute;
    width: 100%;
}

.p-cell {
    display: flex;
    height: 156rpx;
    width: 100%;
    padding: 20rpx 40rpx;
    background-color: #fff;
    border-top: 1px solid #eee;
    margin-bottom: 10rpx;
}
.first{
    border-top: none;
}
.info {
    flex: 4;
    text-align: left;
    margin-left: 20rpx;
    position: relative;
}
.info .h5 {
    margin-top: 20rpx;
    color: #27272f;
    font-size: 28rpx;

}

.info span {
    font-size: 28rpx;
    color: #666;
}
.logo {
    flex: 1;
}
.logo image {
    width: 80%;
    height: 80%;
    border-radius: 50%;
    display: block;
    margin: 10% auto;
}
.usertime {
    position: absolute;
    right: 64rpx;
    bottom: 10rpx;
    font-size: 24rpx;
}
/*备用页面  */
.share-page {
    padding: 300rpx 25%;
    color: #999;
    background: #f8f8f8;
    text-align:center;
}
.share-page  .iconfont {
    font-size: 140rpx;
}
.share-page .p {
    font-size: 14px;
}